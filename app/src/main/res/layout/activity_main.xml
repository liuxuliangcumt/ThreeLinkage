<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.realpower.threelinkage.MainActivity">

    <Button
        android:id="@+id/btnPick"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="选择地址" />

    <com.realpower.threelinkage.view.PickAddressView
        android:id="@+id/pickView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.realpower.threelinkage.view.PickAddressView>
</LinearLayout>
